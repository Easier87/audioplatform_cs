<Window x:Class="audioplatform_cs.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:audioplatform_cs"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="MainWindow" Height="540" Width="960">
    <Border Background="#1b1b1b">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="30*"/>
                <ColumnDefinition Width="35*"/>
                <ColumnDefinition Width="31*"/>
            </Grid.ColumnDefinitions>

            <Rectangle Fill="#292929" Grid.Column="2" />
            <Rectangle Fill="#292929"/>
        </Grid>
    </Border>
</Window>
